<Multi_key> <U> : "Ŭ"

# Portuguese

<Multi_key> <;> : "ç"
<Multi_key> <:> : "Ç"

<Multi_key> <|> <a> : "â"
<Multi_key> <|> <e> : "ê"
<Multi_key> <|> <o> : "ô"
<Multi_key> <|> <A> : "Â"
<Multi_key> <|> <E> : "Ê"
<Multi_key> <|> <O> : "Ô"

<Multi_key> <\> <a> : "ã"
<Multi_key> <\> <o> : "õ"
<Multi_key> <\> <A> : "Ã"
<Multi_key> <\> <E> : "Ẽ"
<Multi_key> <\> <O> : "Õ"

<Multi_key> <]> <a> : "á"
<Multi_key> <]> <e> : "é"
<Multi_key> <]> <i> : "í"
<Multi_key> <]> <o> : "ó"
<Multi_key> <]> <u> : "ú"
