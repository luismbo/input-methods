<Multi_key> <#> <e> : "ě"
<Multi_key> <#> <i> : "ǐ"
<Multi_key> <#> <o> : "ǒ"
<Multi_key> <#> <u> : "ǔ"
<Multi_key> <#> <A> : "Ǎ"
<Multi_key> <#> <E> : "Ě"
<Multi_key> <#> <I> : "Ǐ"
<Multi_key> <#> <O> : "Ǒ"
<Multi_key> <#> <U> : "Ǔ"

# pinyin ǖǘǚǜ ǕǗǙǛ

<Multi_key> <@> <y> : "ǖ"
<Multi_key> <@> <Y> : "Ǖ"
<Multi_key> <#> <y> : "ǚ"
<Multi_key> <#> <Y> : "Ǚ"
<Multi_key> <]> <y> : "ǘ"
<Multi_key> <]> <Y> : "Ǘ"
<Multi_key> <}> <y> : "ǜ"
<Multi_key> <}> <Y> : "Ǜ"

# useful symbols

<Multi_key> <o> : "°"
<Multi_key> <?> : "–"
<Multi_key> </> : "—"

